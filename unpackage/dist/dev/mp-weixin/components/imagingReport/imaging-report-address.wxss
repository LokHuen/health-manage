
.box {
	margin-top: 10px;
	margin-left: auto;
	margin-right: auto;
	height: auto;
	width: 93%;
	background-color: #FFFFFF;
	color: #333333;
	-webkit-border-radius: 5px;
	        border-radius: 5px;
	-webkit-box-shadow: 1px 1px 5px #999999;
	        box-shadow: 1px 1px 5px #999999;
}
.title {
	font-size: 16px;
	color: #333333;
	margin-left: 15px;
	padding-top: 18px;
	height: 15.5px;
	line-height: 15.5px;
}
.sub-title {
	font-size: 13px;
	color: #999999;
	margin-left: 15px;
	margin-top: 10px;
	height: 13.5px;
	line-height: 13.5px;
}
.line {
	background-color: #D2D2D2;
	margin-top: 10px;

	margin-left: 15px;
	margin-right: 15px;
	height: 1px;
}
.agency-input {
	margin-top: 15px;
	margin-left: 15px;
	margin-right: 15px;
	font-size: 16px;
	color: #666666;
}
.bottom-blank {
	padding-bottom: 20px;
}

