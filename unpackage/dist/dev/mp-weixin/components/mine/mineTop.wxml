<view><view class="top"><view data-event-opts="{{[['tap',[['blank',['$event']]]]]}}" catchtap="__e"><block wx:if="{{!isLogin}}"><button class="login-top" open-type="getUserInfo" hover-stop-propagation="{{true}}" data-event-opts="{{[['getuserinfo',[['onGotUserInfo',['$event']]]]]}}" bindgetuserinfo="__e"></button></block></view><image class="avator" src="{{isLogin?userInfo.portrait:imageUrl+'/mine/icon_avator.png'}}" mode="widthFix"></image><view class="name">{{''+(isLogin?userInfo["nickname"]:"点击登录")+''}}</view></view><view style="height:10px;"></view><block wx:for="{{list}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view data-event-opts="{{[['tap',[['goDetail',[index]]]]]}}" class="itemList" bindtap="__e"><view data-event-opts="{{[['tap',[['blank',['$event']]]]]}}" catchtap="__e"><block wx:if="{{!isLogin}}"><button class="i-login-btn" open-type="getUserInfo" data-event-opts="{{[['getuserinfo',[['onGotUserInfo',['$event']]]]]}}" bindgetuserinfo="__e"></button></block></view><image class="listIcon" src="{{item[1]}}" mode="widthFix"></image><view class="listText"><text>{{item[0]}}<text hidden="{{!(index==0&&userInfo.healthDialogue==1&&isLogin)}}" class="red-point"></text></text></view><view hidden="{{!(index==2&&userInfo.unreadOrder==1&&isLogin)}}" class="order-tip">有待评价订单</view></view></block></view>