<view class="container"><view class="list"><block wx:for="{{orderData}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view data-event-opts="{{[['tap',[['itemClick',['$0'],[[['orderData','',index]]]]]]]}}" class="list-item" bindtap="__e"><view class="left"><text class="text">{{item.interpretTypeValue}}</text><text class="order-no">{{'订单号'+' '+item.orderNumber}}</text><text class="date">{{'日期 '+item.createTime}}</text><block wx:if="{{!item.payStatusValue&&item.appraiseStar!=0}}"><text class="rate-text">{{'评价：'+item.appraiseStar+'星服务'}}</text></block><block wx:if="{{item.appraiseVisible==1}}"><view class="rate-box"><text>点击星星评价服务：</text><uni-rate class="rate" vue-id="{{'3c50aaa2-1-'+index}}" max="{{5}}" value="{{0}}" color="#FFBA00" margin="{{2}}" coloactive-color="#FFBA00" size="16" is-fill="{{false}}" data-event-opts="{{[['^change',[['rateChange']]]]}}" bind:change="__e" bind:__l="__l"></uni-rate></view></block></view><view class="right"><text class="price">{{'¥'+' '+(item.payAmount?item.payAmount:0)}}</text><text class="refund">{{item.payStatusValue}}</text></view></view></block></view><block wx:if="{{orderData.length==0}}"><view class="empty-box"><image src="{{imageUrl+'/image/no_data.png'}}"></image><text>无数据</text></view></block><block wx:if="{{showSppraise}}"><view data-event-opts="{{[['touchmove',[['e0',['$event']]]]]}}" class="appraise_mask" catchtouchmove="__e"><view class="appraise_content"><image src="{{imageUrl+'/icon_close.png'}}" data-event-opts="{{[['tap',[['cancelSppraise',['$event']]]]]}}" bindtap="__e"></image><text>{{'我的评价：'+starValue+'星服务'}}</text><text>星级越高代表服务满意度越高</text><text data-event-opts="{{[['tap',[['orderEvaluate',['$event']]]]]}}" bindtap="__e">确定</text></view></view></block></view>