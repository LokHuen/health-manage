
.closebt {
	position: absolute;
	top: 20rpx;
	left: 20rpx;
}
.chatbox{padding-bottom:140rpx;}
.surecloseask{line-height:1;padding:24rpx 0;width:320rpx;margin:50rpx auto 0;color:#fff;background:#007AFF;-webkit-border-radius:10rpx;border-radius:10rpx;}
.boxtitle{font-size:38rpx;line-height:120rpx;}
.endthisask{background:#fff;text-align:center;padding:0 0 50rpx;-webkit-border-radius: 20rpx 20rpx 0 0;border-radius: 20rpx 20rpx 0 0;}
.endthisask view:nth-child(2){color:#666;}
.askfinish{color:rgba(9, 143, 242, 100);}
.doctortime{position: -webkit-sticky;position: sticky;top:0;left:0;right:0;background:#fff;padding:24rpx 30rpx 24rpx 26rpx;display: -webkit-box;display: -webkit-flex;display: flex;margin:0 0rpx;-webkit-box-align:center;-webkit-align-items:center;align-items:center;font-size: 30rpx;z-index:9;border-top:1rpx solid #eee;}
.serviceend{position:absolute;top:0;left:0;right:0;bottom:0;z-index:100;-webkit-box-pack:center;-webkit-justify-content:center;justify-content:center;display:-webkit-box;display:-webkit-flex;display:flex;-webkit-box-align:center;-webkit-align-items:center;align-items:center;background: rgba(255,255,255,0.6);color: #666;}
.time {
  font-size: 28rpx;color:#666;line-height:1;
}
.tipscss{line-height:1.4;text-align:left;background: rgba(39, 147, 236, 0.08);-webkit-border-radius:10rpx;border-radius:10rpx;padding:22rpx;font-size: 28rpx;color: #666666;}
.tipscss .jumpcolor{color:#2793EC;}
