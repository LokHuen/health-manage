<view class="contain"><block wx:for="{{listData}}" wx:for-item="item" wx:for-index="index" wx:key="index"><block wx:if="{{listData.length>0}}"><view class="list"><view class="creat-time">{{''+item.createTime+''}}</view><view data-event-opts="{{[['tap',[['open',['$0'],[[['listData','',index]]]]]]]}}" class="list-box" bindtap="__e"><view class="prelative"><image class="image" src="{{item.portrait}}" mode="aspectFill"></image><block wx:if="{{item.patientRead==0}}"><text class="pageredround"></text></block></view><view class="content-box"><view class="name">{{item.modifier}}</view><view class="content">{{item.title}}</view></view><view class="last-time">{{item.modifyTime}}</view></view></view></block></block><block wx:if="{{listData.length==0}}"><view class="no-data-tip"><image class="no-data-img" src="{{imageUrl+'/image/no_data.png'}}"></image><view>无数据</view></view></block></view>