<view class="contain"><view class="flex phonetab"><view data-event-opts="{{[['tap',[['clickthis',[1]]]]]}}" class="itemlist" bindtap="__e"><view class="{{[chooseindex==1?'on':'']}}" style="position:relative;display:inline-block;"><text>{{"待解读("+consultCount.forCount+")"}}</text><block wx:if="{{consultCount.forUnread==1}}"><view class="red-point"></view></block></view></view><view data-event-opts="{{[['tap',[['clickthis',[2]]]]]}}" class="itemlist" bindtap="__e"><view class="{{[chooseindex==2?'on':'']}}" style="position:relative;display:inline-block;"><text>{{"解读完成("+consultCount.overCount+")"}}</text><block wx:if="{{consultCount.overUnread==1}}"><view class="red-point"></view></block></view></view></view><block wx:if="{{listData.length>0}}"><view class="list"><block wx:for="{{listData}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view data-event-opts="{{[['tap',[['open',['$0'],[[['listData','',index]]]]]]]}}" class="list-box" bindtap="__e"><view class="prelative"><image class="image" src="{{item.portrait}}" mode="aspectFill"></image><block wx:if="{{item.doctorRead==0}}"><text class="pageredround"></text></block></view><view class="content-box"><view class="name">{{item.modifier}}</view><view class="content">{{item.title}}</view></view><view class="last-time">{{item.modifyTime}}</view></view></block></view></block><block wx:if="{{listData.length==0}}"><view class="no-data-tip"><image class="no-data-img" src="{{imageUrl+'/image/no_data.png'}}"></image><view>无数据</view></view></block></view>